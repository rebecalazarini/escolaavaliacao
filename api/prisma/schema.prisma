datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Usuario {
  id            Int    @id @default(autoincrement())
  senha         String @db.VarChar(50)
  email         String @unique
}

model Tarefa {
    id   Int    @id @default(autoincrement())
    número Int
    nome String
}

model Turma {
     id   Int    @id @default(autoincrement())
    número Int
    nome String
}